<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Azure DevOps compliance NuGet.config - Single Azure Artifacts feed only -->
  <packageSources>
    <clear />
    <!-- Primary Azure Artifacts feed for compliance -->
    <add key="azure-artifacts" value="https://pkgs.dev.azure.com/yourorg/_packaging/yourfeed/nuget/v3/index.json" protocolVersion="3" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="azure-artifacts">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
